package Basic41_50;

import java.util.Scanner;

// 1044 : [기초-산술연산] 정수 1개 입력받아 1 더해 출력하기(설명)
public class test44 {
	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		
		int num1 = sc.nextInt();
		int num2 = sc.nextInt();
		
		System.out.println(num1%num2);
		
	}
}
