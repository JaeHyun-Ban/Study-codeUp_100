package Basic31_40;

import java.util.Scanner;

// 1038 : [기초-산술연산] 정수 2개 입력받아 합 출력하기1(설명)
public class test38 {

	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);

		int num = sc.nextInt();//정수 입력받기
		sc.close();
		
		char c = (char) num;//
		
		System.out.println(c);

	}
}
