
package basic51_60;

import java.util.Scanner;

// 1053 : [기초-논리연산] 참 거짓 바꾸기(설명)
public class test53 {
	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);
		
		boolean flag = sc.nextBoolean();
		int num = sc.nextInt();
		sc.close();// 자원아끼기

		// C언어
		System.out.printf("%d", !flag);
	}
}
